    /// <returns></returns>
    private List<string> GetTableColumns(string ContextNamespace)
    {
        var clrEntityType = TraceabilityDB.Model.FindEntityType(ContextNamespace);
        List<string> tableColumnsNames = new List<string>();
        if (clrEntityType != null)
        {          
